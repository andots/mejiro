  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "app",
  "description": "enables the app permissions",
  "windows": ["main"],
  "webviews": ["app"],
  "permissions": [
    "core:default",
    "log:default",
    "window-geometry:default",
    "app-settings:default",
    "user-settings:default",
    "bookmarks:default",
    "app-webview"
  ]
}
