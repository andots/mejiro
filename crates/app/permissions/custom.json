{
  "default": null,
  "permission": [
    {
      "identifier": "app-webview",
      "description": "permissions for the app webview",
      "commands": {
        "allow": [
          "get_app_webview_bounds",
          "navigate_webview_url",
          "get_external_webview_title",
          "set_external_webview_bounds",
          "show_external_webview",
          "hide_external_webview"
        ],
        "deny": []
      }
    },
    {
      "identifier": "external-webview",
      "description": "permissions for the external webview",
      "commands": {
        "allow": ["send_page_title", "send_page_url"],
        "deny": []
      }
    }
  ]
}
